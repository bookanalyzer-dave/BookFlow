# Condition Assessor Agent Environment Variables
# Copy this file to .env.yaml for Cloud Functions deployment

# GCP Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
VERTEX_AI_LOCATION=europe-west1

# Vertex AI Models
CONDITION_MODEL_NAME=imagetext@001

# Feature Flags
LLM_MANAGER_ENABLED=true

# Logging
LOG_LEVEL=INFO
ENABLE_CLOUD_LOGGING=true

# Environment
ENVIRONMENT=development  # development, alpha, production